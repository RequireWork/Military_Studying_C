using System;

namespace HelloWorld
{
  class Program
  {
    static void Main()
    {
      Console.WriteLine("YJ!");    
      string howsyourday = Console.ReadLine();
      Console.WriteLine($"The name is {howsyourday}");
     }
  }
}

