#include <stdio.h>
#include <string.h>

int main(){
  int counter1[] = {0,0,0,0};
  int counter2[] = {0,0,0,0};
  char s1[] = {'d','b','b', ,'c','c','c','c','c','a','a','c','b',' ','c','d','b','a','b','a','b','d','c','d','a','b',' ','d','c','d','a','d'};
  char s2[] = {'b','b','b','c','c',' ','b','d','d','d','c','c','c','c','a','d',' ','c','d','b','b','a','a','a','c','a','c','c','d','a','b','d','d'};

  for(int i = 0; i < strlen(s1); i++){
    if(s1[i] == 'a'){
      counter1[0]++;
    }
    else if(s1[i] == 'b'){
      counter1[1]++;
    }
    else if(s1[i] == 'd'){
      counter1[2]++;
    }
    else if(s1[i] == 'c'){
      counter1[3]++;
    }
  }
}
